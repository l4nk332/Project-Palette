{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [0],
    "function-paren-newline": ["error", "consistent"],
    "object-curly-spacing": ["error", "never"],
    "arrow-body-style": ["error", "as-needed"],
    "arrow-parens": ["error", "as-needed"],
    "jsx-a11y/anchor-is-valid": "off"
  },
  "env": {
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint"
}
